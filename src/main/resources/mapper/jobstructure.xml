<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="deal.controller.model.JobStructure">
  <select id="list" resultType="deal.controller.model.JobStructure">
    select
    id
    ,nodeId
    ,parentId
    ,isVisited
    ,isType
    ,jobname
    from jobstructure
  </select>

</mapper>
